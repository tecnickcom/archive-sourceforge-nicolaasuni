		<name>general</name>
		<description>tag part of the current standard and not deprecated</description>
	</item>
	<item id="2">
		<name>browser specific</name>
		<description>element specific for a particular browser</description>
	</item>
	<item id="3">
		<name>deprecated</name>
		<description>deprecated element</description>
	</item>
	<item id="4">
		<name>obsolete</name>
		<description>obsolete tag</description>
	</item>
</jxedata>